    "lucide-react": "^0.542.0",
    "monaco-editor": "^0.53.0",
    "next": "^14.2.18",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "prisma": "^6.15.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
